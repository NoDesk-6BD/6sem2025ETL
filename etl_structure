    ├── etl.py                # Script principal de orquestração
    ├── extractor.py          # Lógica de extração de dados do SQL Server
    ├── loader.py             # Lógica de carga de dados no PostgreSQL
    └── transformer.py        # Lógica de LGPD e normalização dos dados
